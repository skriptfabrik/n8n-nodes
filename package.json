    "form-data": "^4.0.4",
    "jest": "30.2.0",
    "jest-environment-node": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "jest-util": "30.2.0",
    "jsbarcode": "^3.12.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "n8n": "1.117.3",
    "n8n-nodes-base": "^1.87.0",
    "n8n-workflow": "^1.86.0",
    "nx": "22.0.1",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "ts-jest": "29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.2",
    "verdaccio": "6.2.1"
  },
  "engines": {
    "node": "22.18.0",
    "pnpm": "^10.15.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "22.18.0",
      "onFail": "warn"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "^10.15.1",
      "onFail": "warn"
    }
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "packageManager": "pnpm@10.15.1"
}
