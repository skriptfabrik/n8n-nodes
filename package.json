    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@nx/devkit": "22.0.1",
    "@nx/eslint": "22.0.1",
    "@nx/eslint-plugin": "22.0.1",
    "@nx/jest": "22.0.1",
    "@nx/js": "22.0.1",
    "@nx/node": "22.0.1",
    "@nx/workspace": "22.0.1",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.13.5",
    "@swc/helpers": "0.5.17",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "~22.18.11",
    "canvas": "^3.2.0",
