{
  "name": "@skriptfabrik/n8n-nodes-moco",
  "version": "0.5.0",
  "description": "MOCO community nodes for n8n",
  "license": "MIT",
  "homepage": "https://github.com/skriptfabrik/n8n-nodes/blob/main/nodes/moco/README.md",
  "bugs": "https://github.com/skriptfabrik/n8n-nodes/issues",
  "keywords": [
    "moco",
    "n8n",
    "n8n-community-node",
    "n8n-community-node-package"
  ],
  "author": {
    "name": "skriptfabrik",
    "email": "info@skriptfabrik.com"
  },
  "repository": "skriptfabrik/n8n-nodes.git",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev --external-n8n --custom-user-folder=$N8N_USER_FOLDER",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "prepublishOnly": "n8n-node prerelease",
    "release": "release-it --increment $(node ../../tools/scripts/conventional-bump.mjs)"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/MocoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Moco/Moco.node.js",
      "dist/nodes/Moco/MocoTrigger.node.js"
    ]
  },
  "dependencies": {
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
